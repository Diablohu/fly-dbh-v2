{
    "name": "fly-dbh-v2",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "start": "node ./start.mjs",
        "dev": "astro dev --host --allowed-hosts local-dev.fly-dbh.com",
        "dev:test": "astro dev --mode test",
